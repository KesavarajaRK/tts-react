{
  "compilerOptions": {
    "target": "es6",
    "jsx": "react-jsx",
    "module": "Node16",
    "rootDir": "src",
    "declaration": true,
    "outDir": "dist",
    "isolatedModules": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "src"
  ]
}
